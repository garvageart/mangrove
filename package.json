{
	"name": "mangrove",
	"version": "1.0.1",
	"description": "Mangrove is a program designed to manage and sync data between Webflow and the local database",
	"private": true,
	"type": "module",
	"scripts": {
		"lint": "eslint",
		"test": "jest",
		"build-app": "node scripts/esbuild.js",
		"build": "npm run build-app && npm run l2w-build",
		"l2w-dev": "vite dev --host",
		"l2w-build": "vite build",
		"l2w-preview": "vite preview",
		"l2w-check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"l2w-check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"author": "Les <hello@lesis.online>",
	"license": "MIT",
	"dependencies": {
		"@fastify/cors": "^8.3.0",
		"@fastify/static": "^6.10.1",
		"@google-cloud/storage": "^7.1.0",
		"chalk": "^5.2.0",
		"dayjs": "^1.11.7",
		"dotenv": "^16.0.3",
		"exifreader": "^4.12.0",
		"fastify": "^4.14.0",
		"inquirer": "^9.1.5",
		"make-synchronous": "^1.0.0",
		"mkdirp": "^2.1.6",
		"mongoose": "^7.0.0",
		"quill": "^1.3.7",
		"quill-image-drop-module": "^1.0.3",
		"quill-image-uploader": "^1.3.0",
		"quill-resize-image": "^1.0.4",
		"sharp": "^0.31.3",
		"socket.io": "^4.7.2",
		"socket.io-client": "^4.7.2",
		"spotify-web-api-node": "^5.0.2",
		"twitter-api-v2": "^1.14.2",
		"webflow-api": "^1.2.2"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.24.1",
		"@types/inquirer": "^9.0.3",
		"@types/jest": "^29.5.1",
		"@types/jquery": "^3.5.18",
		"@types/quill": "^2.0.11",
		"@types/sharp": "^0.31.1",
		"@types/spotify-api": "^0.0.21",
		"@types/spotify-web-api-node": "^5.0.7",
		"@typescript-eslint/eslint-plugin": "^5.57.0",
		"@typescript-eslint/parser": "^5.57.0",
		"esbuild": "^0.17.14",
		"eslint": "^8.37.0",
		"jest": "^29.5.0",
		"nodemon": "^3.0.1",
		"sass": "^1.67.0",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.1",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.1",
		"tsx": "^3.12.6"
	}
}
